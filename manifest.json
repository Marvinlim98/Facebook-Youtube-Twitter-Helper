{
  "manifest_version": 2,
  "name": "Facebook/Youtube/Twitter Helper ",
  "version": "1.1",
  "description": "",
  "icons": 
        {
        "48": "48x48.ico",
        "96": "96x96.ico"
        },
  "content_scripts": 
        [
            {
            "matches": ["*://*.youtube.com/*"],
            "js": ["youtube.js"]
            },
			{
			"matches": ["*://*.facebook.com/*"],
            "js": ["facebook.js"]
			},
			{
			"matches": ["*://twitter.com/*"],
            "js": ["twitter.js"]
			}
        ]
}